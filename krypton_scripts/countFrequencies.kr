{
    def countFrequencies(lst) {

        def countHelper() {
            counts = {};
            for(i = 0; i < len(lst); i = i + 1) {
                counts[lst[i]] = counts[lst[i]] + 1;
            }
            return counts;
        }

        counts = countHelper();
        for(i = 0; i < len(lst); i = i + 1) {
            print("Element " + lst[i] + " -> " + counts[lst[i]]);
        }
    }


    ourList = [1, 2, 3, 2, 1, 6, 7];
    countFrequencies(ourList);

}