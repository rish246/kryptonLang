{
    def countFrequencies(lst) {

        def countHelper() {
            counts = {};
            for(value in lst) {
                counts[value] = counts[value] + 1;
            }
            return counts;
        }

        counts = countHelper();
        for(entry in counts) {
            print("Element " + entry[0] + " -> " + entry[1]);
        }
    }


    ourList = [1, 2, 3, 2, 1, 6, 7];
    countFrequencies(ourList);

}