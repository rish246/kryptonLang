class T {
    def init(val) {
        self.val = val;
        self.next = null;
    }

    def update(val) {
        if (self.next == null) { # Right here () -> () ??
            self.next = new T(val);
        } else {
            temp = self;
            while (temp.next != null) {
                temp = temp.next;
            }
            temp.next = new T(val);
        }
    }
};

t = new T(4);
t.update(0);
t.update(1);
t.update(2);
t.update(3);

